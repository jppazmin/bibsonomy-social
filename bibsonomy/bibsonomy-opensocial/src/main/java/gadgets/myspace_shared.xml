<Content type="html" view="home"><![CDATA[

<script>
var preloadedImage = new Image();

_IG_RegisterOnloadHandler(
    function() {
      var prefs = new _IG_Prefs(__MODULE_ID__);
      _gm = new _GadgetMaker(prefs);
      var hasPicture = false;
      var pictureLink = prefs.getString('pic');
      if (pictureLink.substring(0, 7) != 'http://') {
        pictureLink = 'http://' + pictureLink;
      }
      if (pictureLink != 'http://') {
        preloadedImage.src = pictureLink;
        _gel('pic_box').innerHTML = '<img id="pic" src="' +
                                    _hesc(pictureLink) + '">';
        _gel('pic_box').style.display = 'block';
        hasPicture = true;
      }
      var pic_elem =_gel('pic');
      if (pic_elem) {
        pic_elem.style.border = '1px solid ' + _gm._borderColor;
      }
      if (prefs.getString('soap') == '') {
        _gel('soap').style.display = 'none';
      } else {
        _gel('q5').innerHTML = _hesc(prefs.getString('soap_label')) + ':';
        _gel('q5').style.color = _gm._fontColor;
        _gel('a5').innerHTML = _hesc(prefs.getString('soap'));
      }
      for (var i = 1; i <= 4; i++) {
        if (prefs.getString('q' + i) == '' || prefs.getString('a' + i) == '') {
          _gel('' + i).style.display = 'none';
        } else {
          _gel('q' + i).innerHTML = _hesc(prefs.getString('q' + i));
          _gel('q' + i).style.color = _gm._fontColor;
          _gel('a' + i).innerHTML = _hesc(prefs.getString('a' + i));
        }
      }
      _gm._setBackground();
      _gm._addFrameIfPreview();
      _gm._enforceMinimumHeight(hasPicture);
      _IG_AdjustIFrameHeight();
    }
);
</script>
